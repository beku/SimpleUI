<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object id="sGTK_GLADE_MAIN_WINDOW_NAME" class="GtkWindow">
    <property name="default_height">680</property>
    <property name="default_width">500</property>
    <property name="can_focus">False</property>
    <property name="title" translatable="yes">SimpleUI</property>
    <signal name="destroy" handler="gtk_main_quit" swapped="no"/>
    <child>
      <object id="Vbox_MainVBox" class="GtkBox">
        <property name="width_request">500</property>
        <property name="height_request">680</property>
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="orientation">vertical</property>
        <child>
          <object id="BottomPaneHBox" class="GtkBox">
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="homogeneous">True</property>
            <child>
              <placeholder/>
            </child>
            <child>
              <placeholder/>
            </child>
            <child>
              <object id="BtnClose" class="GtkButton">
                <property name="label">gtk-close</property>
                <property name="use_action_appearance">False</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">True</property>
                <property name="halign">end</property>
                <property name="use_stock">True</property>
                <signal name="clicked" handler="gtk_main_quit" swapped="no"/>
              </object>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">1</property>
            <property name="pack_type">end</property>
          </packing>
        </child>
        <child>
          <object id="scrolledwindow_ContentArea" class="GtkScrolledWindow">
            <property name="visible">True</property>
            <property name="shadow_type">in</property>
            <property name="can_focus">True</property>
            <child>
              <object id="viewport_scrolledwindow_ContentArea" class="GtkViewport">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <child/>
              </object>
              <object id="GtkNotebook_Main" class="GtkNotebook">
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <child>
                  <object id="content_Welcome Page" class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="shadow_type">in</property>
                  </object>
                </child>
                <child type="tab">
                  <object id="label_Welcome Page" class="GtkLabel">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="label" translatable="yes">Welcome Page</property>
                  </object>
                  <packing>
                    <property name="position">0</property>
                    <property name="tab_fill">False</property>
                  </packing>
                </child>
                <child>
                  <object id="content_Personal Information" class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="shadow_type">in</property>
                  </object>
                </child>
                <child type="tab">
                  <object id="label_Personal Information" class="GtkLabel">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="label" translatable="yes">Personal Information</property>
                  </object>
                  <packing>
                    <property name="position">1</property>
                    <property name="tab_fill">False</property>
                  </packing>
                </child>
                <child>
                  <object id="content_Projects" class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="shadow_type">in</property>
                  </object>
                </child>
                <child type="tab">
                  <object id="label_Projects" class="GtkLabel">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="label" translatable="yes">Projects</property>
                  </object>
                  <packing>
                    <property name="position">2</property>
                    <property name="tab_fill">False</property>
                  </packing>
                </child>
                <child>
                  <object id="content_Preferences" class="GtkScrolledWindow">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="shadow_type">in</property>
                  </object>
                </child>
                <child type="tab">
                  <object id="label_Preferences" class="GtkLabel">
                    <property name="visible">True</property>
                    <property name="can_focus">False</property>
                    <property name="label" translatable="yes">Preferences</property>
                  </object>
                  <packing>
                    <property name="position">3</property>
                    <property name="tab_fill">False</property>
                  </packing>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="expand">True</property>
            <property name="fill">True</property>
            <property name="position">0</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
